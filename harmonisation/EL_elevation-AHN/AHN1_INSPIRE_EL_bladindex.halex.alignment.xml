            </property>
        </target>
        <complexParameter name="script">
            <core:text xmlns:core="http://www.esdi-humboldt.eu/hale/core" xml:space="preserve">
//returns the list of found geometries (each wrapped in a GeometryProperty)&#xD;
def prop = _.geom.find(geom.p.first())&#xD;
&#xD;
def geoms&#xD;
&#xD;
if(prop) {&#xD;
	geoms = prop.getGeometry()&#xD;
}&#xD;
&#xD;
def x_left&#xD;
def y_low&#xD;
&#xD;
geoms.getCoordinates().each{ geo -&gt;&#xD;
	if (!x_left){&#xD;
		x_left = geo.x&#xD;
	}&#xD;
	&#xD;
	else if (geo.x &lt; x_left){&#xD;
		x_left = geo.x&#xD;
	}&#xD;
}&#xD;
&#xD;
geoms.getCoordinates().each{ geo -&gt;&#xD;
	if(!y_low){&#xD;
		y_low = geo.y&#xD;
	}&#xD;
	&#xD;
	else if (geo.y &lt; y_low){&#xD;
		y_low = geo.y&#xD;
	}&#xD;
}&#xD;
&#xD;
if(x_left &amp;&amp; y_low){&#xD;
	def x_right = (x_left as double) + 5000&#xD;
	def y_high = (y_low as double) + 6250&#xD;
&#xD;
	return "https://geodata.nationaalgeoregister.nl/ahn3/wcs?VERSION=2.0.1&amp;SERVICE=WCS&amp;REQUEST=getcoverage&amp;COVERAGEID=" + _project.vars.COVERAGE_ID + "&amp;FORMAT=image/tiff&amp;CRS=EPSG:28992&amp;SUBSET=x(${x_left},${x_right})&amp;SUBSET=y(${y_low},${y_high})"&#xD;
}
</core:text>
        </complexParameter>
        <parameter value="true" name="variablesAsInstances"/>
    </cell>
